{
  "name": "mobile-visual-regression",
  "version": "3.0.0",
  "description": "Mobile visual regression testing tool - compare Figma designs with mobile app screenshots",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "compare": "ts-node src/cli/compare.ts",
    "clean": "rm -rf storage/uploads/* storage/output/* && rm -f storage/database.sqlite",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "keywords": [
    "mobile",
    "visual-regression",
    "testing",
    "figma",
    "image-comparison",
    "ios",
    "android"
  ],
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.0",
    "cors": "^2.8.5",
    "sharp": "^0.33.1",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "uuid": "^9.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/cors": "^2.8.17",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/uuid": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
